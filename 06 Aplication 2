# -----------
# LL fit ---
# -----------

 fit <- optim(par=c( 0.01,0.5),fn=loglik,method=c("L-BFGS-B"),hessian=TRUE,lower=c(1e-12,1e-12),upper=c(Inf,Inf),x=x)
 fit$par 
 fit$value 
 AIC=-2*(- 1157.527)+2*2
 AIC
 BIC=-2*(- 1157.527)+2*log(300)
 BIC
 hessiana <- fit$hessian
 vcov_matrix <- solve(hessiana)
 error_estandar <- sqrt(diag(vcov_matrix))
 error_estandar

